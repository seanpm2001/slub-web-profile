plugin.tx_slubwebprofile {
  mvc {
    callDefaultActionIfActionCantBeResolved = 1
    throwPageNotFoundExceptionIfActionCantBeResolved = 1
  }

  features {
    skipDefaultArguments = 1
  }

  settings {
    storagePid = {$plugin.tx_slubwebprofile.settings.storagePid}

    api {
      path {
        eventList {
          0 = {$plugin.tx_slubwebprofile.settings.api.path.eventList.0}
          1 = {$plugin.tx_slubwebprofile.settings.api.path.eventList.1}
        }
      }
    }
  }
}

page.10 {
  templateRootPaths.25 = EXT:slub_web_profile/Resources/Private/Templates/
  partialRootPaths.25 = EXT:slub_web_profile/Resources/Private/Partials/
  layoutRootPaths.25 = EXT:slub_web_profile/Resources/Private/Layouts/
}
